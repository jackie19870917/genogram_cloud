<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.genogram.mapper.ChiIndexFundMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.genogram.entity.ChiIndexFund">
        <id column="id" property="id"/>
        <result column="site_id" property="siteId"/>
        <result column="remain" property="remain"/>
        <result column="pay_num" property="payNum"/>
        <result column="unuse_amount" property="unuseAmount"/>
        <result column="pay_online" property="payOnline"/>
        <result column="pay_underline" property="payUnderline"/>
        <result column="pay_genogram" property="payGenogram"/>
        <result column="create_time" property="createTime"/>
        <result column="create_user" property="createUser"/>
        <result column="update_time" property="updateTime"/>
        <result column="update_user" property="updateUser"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, site_id AS siteId, remain, pay_num AS payNum, unuse_amount AS unuseAmount, pay_online AS payOnline, pay_underline AS payUnderline, pay_genogram AS payGenogram, create_time AS createTime, create_user AS createUser, update_time AS updateTime, update_user AS updateUser
